<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="#FFC006"/>
                    <corners android:radius="5dp"/>
                </shape>
            </item>
            <item android:bottom="9dp"
                  android:drawable="@drawable/ic_launcher"
                  android:left="9dp"
                  android:right="9dp"
                  android:top="9dp"/>
        </layer-list>
    </item>
    <item>
        <layer-list>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="#FFF"/>
                    <stroke android:width="1dp" android:color="#FFC006"/>
                    <corners android:radius="5dp"/>
                </shape>
            </item>
            <item
                android:bottom="9dp"
                android:drawable="@drawable/ic_launcher"
                android:left="9dp"
                android:right="9dp"
                android:top="9dp"/>
        </layer-list>
    </item>
</selector>